{
  "name": "openmarket-api",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./*"
  },
  "nodemonConfig": {
    "ignore": [
      "swagger-output.json"
    ],
    "exec": "npm run prestart && node"
  },
  "scripts": {
    "prestart": "node ./swagger.js",
    "start": "cross-env NODE_ENV=local node -r dotenv/config ./bin/www-https.js",
    "dev": "cross-env NODE_ENV=development nodemon -r dotenv/config ./bin/www-https.js",
    "local": "cross-env NODE_ENV=local nodemon -r dotenv/config ./bin/www-https.js",
    "dbinit": "cross-env NODE_ENV=local node -r dotenv/config ./samples/dbinit.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^6.2.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "shortid": "^2.2.16",
    "swagger-autogen": "^2.23.6",
    "swagger-ui-express": "^5.0.0",
    "tracer": "^1.3.0"
  }
}
